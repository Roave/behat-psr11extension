{
    "name": "roave/behat-psr11extension",
    "description": "PSR-11 Container extension for Behat",
    "type": "library",
    "require": {
        "php": "~8.0.0 || ~8.1.0",
        "behat/behat": "^3.11.0",
        "psr/container": "^1.1.2",
        "symfony/dependency-injection": "^6.0.13"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.2.1",
        "laminas/laminas-servicemanager": "^3.19.0",
        "phpunit/phpunit": "^9.5.26"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Roave\\BehatPsrContainer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RoaveFeatureTest\\BehatPsrContainer\\": "features/bootstrap/",
            "RoaveTest\\BehatPsrContainer\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "8.0.99"
        },
        "sort-packages": true
    }
}
