{
    "name": "roave/behat-psr11extension",
    "description": "PSR-11 Container extension for Behat",
    "type": "library",
    "require": {
        "php": "^7.1 || ^8.0",
        "behat/behat": "^3.4",
        "psr/container": "^1.0",
        "symfony/dependency-injection": "^2.7 || ^3.0 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "doctrine/coding-standard": "^8.2",
        "laminas/laminas-servicemanager": "4.0.x-dev",
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Titcumb",
            "email": "james@asgrim.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Roave\\BehatPsrContainer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RoaveFeatureTest\\BehatPsrContainer\\": "features/bootstrap/",
            "RoaveTest\\BehatPsrContainer\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
